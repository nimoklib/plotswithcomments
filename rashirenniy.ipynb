    "def gcdlarge(a, b):\n",
    "    if a == 0:\n",
    "        return (b, 0, 1)\n",
    "    else:\n",
    "        d, x, y = gcdlarge(b % a, a)\n",
    "    return (d, y - (b // a) * x, x)\n",
    "q = gcdlarge(6, 125)\n",
    "print(q[0],'=(a *',q[1],') + (b *',q[2],')')"
   ]
  }
